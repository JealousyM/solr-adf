<?xml version="1.0" encoding="windows-1251" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="solr-adf-btf">
    <default-activity>solr</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>solrBean</managed-bean-name>
      <managed-bean-class>by.perevertkin.solr.view.beans.SolrBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="solr">
      <page>/solr.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
